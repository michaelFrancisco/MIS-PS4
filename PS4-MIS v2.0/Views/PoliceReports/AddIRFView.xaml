                    <TextBox x:Name="Suspect_rank" Grid.Row="17" Grid.Column="1" FontSize="16" Width="200" Margin="5" Background="#FFDEECFF" />
                </Grid>
            </TabItem>
        </TabControl>
        <Button x:Name="cancelButton" HorizontalAlignment="Left" Width="206" Margin="5" Content="Cancel" Background="#FF1F4287" Foreground="#FFFFF6F6" BorderBrush="#FF1F4287" Grid.Row="1" />
        <Button x:Name="saveasPDFButton" HorizontalAlignment="Center" Width="206" Margin="5" Content="Save as PDF" Background="#FF1F4287" Foreground="#FFFFF6F6" BorderBrush="#FF1F4287" Grid.Row="1" />
        <Button x:Name="saveButton" HorizontalAlignment="Right" Width="206" Margin="5" Content="Save" Background="#FF1F4287" Foreground="#FFFFF6F6" BorderBrush="#FF1F4287" Grid.Row="1" />
    </Grid>
</Window>